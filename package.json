  "devDependencies": {
    "@types/fuzzyset.js": "^0.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/ws": "^8.18.1",
    "@vostrnad/eslint-config-jest": "^2.0.0",
    "@vostrnad/eslint-config-node": "^2.0.0",
